<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.structured.sa.mapper.OrganizationMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.structured.sa.entity.Organization">
        <id column="id" property="id"/>
        <result column="name" property="name"/>
        <result column="address" property="address"/>
        <result column="domain" property="domain"/>
        <result column="introduce" property="introduce"/>
        <result column="admin_name" property="adminName"/>
        <result column="admin_phone" property="adminPhone"/>
        <result column="create_time" property="createTime"/>
        <result column="organization_id" property="organizationId"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id
        , name, address, domain, introduce, admin_name, admin_phone, create_time, organization_id
    </sql>

</mapper>
