<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.structured.sa.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.structured.sa.entity.User">
        <id column="id" property="id"/>
        <result column="username" property="username"/>
        <result column="domain_username" property="domainUsername"/>
        <result column="password" property="password"/>
        <result column="nick_name" property="nickName"/>
        <result column="photo" property="photo"/>
        <result column="email" property="email"/>
        <result column="phone" property="phone"/>
        <result column="is_enabled" property="enabled"/>
        <result column="is_unlocked" property="unlocked"/>
        <result column="is_deleted" property="deleted"/>
        <result column="create_time" property="createTime"/>
        <result column="create_by" property="createBy"/>
        <result column="update_time" property="updateTime"/>
        <result column="update_by" property="updateBy"/>
        <result column="organization_id" property="organizationId"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id
        , username, domain_username, password, nick_name, photo, email, phone, is_enabled, is_unlocked, is_deleted, create_time, create_by, update_time, update_by, organization_id
    </sql>

</mapper>
